<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="main_pane" prefHeight="600.0" prefWidth="850.0" styleClass="main-pane" stylesheets="@../style/main-style.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.mindlauncher.controls.GameController">
    <Pane prefHeight="600.0" prefWidth="250.0" styleClass="buttons-pane" stylesheets="@../style/main-style.css">
        <Button fx:id="ankrButton" layoutX="64.0" layoutY="124.0" mnemonicParsing="false" onAction="#initialize" styleClass="launcher-buttons" stylesheets="@../style/main-style.css" text="Ankr - Decay">
            <font>
                <Font name="Book Antiqua" size="14.0" />
            </font>
        </Button>
        <Button fx:id="vortexButton" layoutX="64.0" layoutY="275.0" mnemonicParsing="false" onAction="#initialize" styleClass="launcher-buttons" stylesheets="@../style/main-style.css" text="Velocity Vortex">
            <font>
                <Font name="Book Antiqua" size="14.0" />
            </font>
        </Button>
        <Button fx:id="exitButton" layoutX="64.0" layoutY="424.0" mnemonicParsing="false" onAction="#initialize" styleClass="launcher-buttons" stylesheets="@../style/main-style.css" text="Exit">
            <font>
                <Font name="Book Antiqua" size="14.0" />
            </font>
        </Button>
    </Pane>
    <Label layoutX="411.0" layoutY="138.0" styleClass="launcher-labels" stylesheets="@../style/main-style.css" text="Welcome to Mind Launcher">
        <font>
          <Font name="Book Antiqua" size="14.0" />
        </font>
    </Label>
</AnchorPane>
