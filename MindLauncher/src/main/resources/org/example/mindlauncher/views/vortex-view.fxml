<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="main_pane" prefHeight="600.0" prefWidth="850.0" styleClass="main-pane" stylesheets="@../style/main-style.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.mindlauncher.controls.VortexController">
    <Pane layoutY="6.0" prefHeight="600.0" prefWidth="250.0" styleClass="buttons-pane" stylesheets="@../style/main-style.css" AnchorPane.topAnchor="0.0">
        <Button fx:id="mainButton" layoutX="99.0" layoutY="167.0" mnemonicParsing="false" onAction="#initialize" styleClass="launcher-buttons" stylesheets="@../style/main-style.css" text="Main">
            <font>
                <Font name="Book Antiqua" size="14.0" />
            </font>
        </Button>
        <Button fx:id="exitButton" layoutX="98.0" layoutY="393.0" mnemonicParsing="false" onAction="#initialize" styleClass="launcher-buttons" stylesheets="@../style/main-style.css" text="Exit">
            <font>
                <Font name="Book Antiqua" size="14.0" />
            </font>
        </Button>
    </Pane>
    <Button fx:id="launchButton" layoutX="472.0" layoutY="417.0" mnemonicParsing="false" onAction="#initialize" styleClass="launcher-buttons" stylesheets="@../style/main-style.css" text="Velocity Vortex">
        <font>
            <Font name="Book Antiqua" size="14.0" />
        </font>
    </Button>
    <Label layoutX="327.0" layoutY="98.0" styleClass="launcher-labels" stylesheets="@../style/main-style.css" text="Welcome to Velocity Vortex">
        <font>
            <Font name="Book Antiqua" size="14.0" />
        </font>
    </Label>
    <Label layoutX="327.0" layoutY="191.0" styleClass="launcher-labels" stylesheets="@../style/main-style.css" text="Press the button for playing the game">
        <font>
            <Font name="Book Antiqua" size="14.0" />
        </font>
    </Label>
</AnchorPane>
